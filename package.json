{
  "name": "react-helmet",
  "description": "A document head manager for React",
  "version": "7.0.0",
  "license": "MIT",
  "main": "dist/Helmet.js",
  "module": "dist/Helmet.esm.js",
  "typings": "dist/Helmet.d.ts",
  "author": "NFL <engineers@nfl.com>",
  "contributors": [
    "Chris Welch <chris.welch@nfl.com>",
    "Robert dela Victoria <robert.delavictoria@nfl.com>"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nfl/react-helmet"
  },
  "keywords": [
    "react-helmet",
    "nfl",
    "react",
    "document",
    "head",
    "title",
    "meta",
    "link",
    "script",
    "base",
    "noscript",
    "style"
  ],
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "dependencies": {
    "@types/sinon": "^9.0.0",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2",
    "react-fast-compare": "^2.0.4",
    "react-side-effect": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/object-assign": "^4.0.30",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "husky": "^4.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sinon": "^9.0.2",
    "tsdx": "^0.13.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsdx build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "prepublish": "npm run build",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {}
}
